{
    "bgKey": "bg_shop_inside",
    "lines": [
      {
        "name": "アヤ",
        "text": "……静かに負けた。最悪",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "静かなの助かる。騒がれると疲れる",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "……やっぱり同類だね。目が死んでる",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "生きてるだけ偉いってやつ",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "冗談じゃない",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "ここで止まる気ないでしょ",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "当たり前",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "なら、これだけ",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "カレンは“勝ち方”を変えない",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "同じ土俵で勝とうとすると、絶対に負ける",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "じゃあどうする",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "あんたは、崩せる",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "身内だからじゃない",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "あんたが、勝ち方を変えられるから",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "……了解",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "次はちゃんと折る、とか言ってたけど",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "折ってほしいのは、私じゃない",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "アヤ",
        "text": "……行って。勝って",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "aya_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      }
    ]
  }
  