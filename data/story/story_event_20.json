{
    "bgKey": "bg_shop_inside",
    "lines": [
      {
        "name": "ボーイ",
        "text": "指名が20になった。この辺から本気で見られる",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "boy_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "やっと同じ目線か",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "boy_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "……あなたがレイさん",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "そうだけど",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "同い年って聞いた",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "だから？",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "だから、簡単に上行かれると困るの",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "この店、年配のお客さん多いでしょ",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "ちゃんと話聞いて、ちゃんと敬って",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "そういうの、私の得意分野だから",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "なるほど。清楚系",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "そう見えるでしょ",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "エリ",
        "text": "でも、負ける気はない",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "ボーイ",
        "text": "エリは同世代の壁だ。甘く見ると持ってかれる",
        "chars": [
          { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
          { "key": "boy_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "ちょうどいい",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      },
      {
        "name": "レイ",
        "text": "同い年なら、遠慮いらないし",
        "chars": [
            { "key": "rei_normal", "x": 380, "y": 520, "scale": 0.5 },
            { "key": "eri_normal", "x": 900, "y": 520, "scale": 0.5 }
        ]
      }
    ]
  }
  